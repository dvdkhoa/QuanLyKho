@model Dictionary<string, string>
@{
    ViewBag.title = "Edit detailed configurations";
}



@section Styles {
    <link rel="stylesheet" href="~/admin/plugins/datatables-bs4/css/dataTables.bootstrap4.min.css">
    <link rel="stylesheet" href="~/admin/plugins/datatables-responsive/css/responsive.bootstrap4.min.css">
    <link rel="stylesheet" href="~/admin/plugins/datatables-buttons/css/buttons.bootstrap4.min.css">
    <!-- SweetAlert2 -->
    <link rel="stylesheet" href="~/admin/plugins/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css">
}


<div class="content mx-2 pb-4">
    <div class="d-flex justify-content-between ">
        <p>
            <a href="javascript:void(0);" onclick="history.go(-1);">Back to Details</a>
        </p>
        <p>
            <a asp-action="EditConfigs" asp-route-id="@ViewBag.productId">Edit</a>
        </p>
    </div>
    <table class="table table-bordered" id="table">
        <tbody>
            @if (Model!.Count > 0)
            {
                foreach (var config in Model)
                {
                    <tr>
                        <td class="w-25">@config.Key</td>
                        <td>@Html.Raw(config.Value)</td>
                    </tr>
                }
            }
        </tbody>
    </table>
</div>